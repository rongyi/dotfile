[
  {
    "bindings": {
      "ctrl-cmd-p": "editor::FindAllReferences"
    }
  },
  {
    "context": "ProjectPanel",
    "bindings": {
      "cmd-n": "zed::NoAction"
    }
  },
  {
    "context": "Editor && vim_mode == normal",
    "bindings": {
      "ctrl-a": "editor::MoveToBeginningOfLine",
      "ctrl-e": "editor::MoveToEndOfLine"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "cmd-;": "editor::ToggleComments",
      "ctrl-t": "zed::NoAction",
      "ctrl-w": "editor::DeleteToPreviousSubwordStart"
    }
  },
  {
    "context": "Pane",
    "bindings": {
      "ctrl-t": "pane::GoBack"
    }
  },
  {
    "context": "Editor && (showing_code_actions || showing_completions)",
    "bindings": {
      "cmd-p": "editor::ContextMenuPrev",
      "cmd-n": "editor::ContextMenuNext"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-cmd-y": "terminal_panel::ToggleFocus"
    }
  }
]
